{
  "name": "bozdoz.com",
  "version": "1.0.0",
  "description": "The portfolio of Benjamin J DeLong",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=development && npm run start:babel",
    "start:babel": "babel-node ./src/dev-server.js",
    "start:production": "npm run build && pm2 start ./src/server.min.js --name='bozdoz.com'",
    "clean": "sudo rm -f ./public/css/main.css && sudo rm ./public/js/main.js",
    "restart": "npm run build && pm2 restart bozdoz.com && npm run purge && service nginx restart",
    "purge": "sudo rm -R ../cache",
    "build": "npm install && export NODE_ENV=production && webpack -p",
    "test": "jest"
  },
  "keywords": [
    "bozdoz",
    "benjamin",
    "delong",
    "web",
    "developer",
    "halifax",
    "programmer"
  ],
  "author": "Benjamin J DeLong <howaboutben@gmail.com> (https://bozdoz.com)",
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "front-matter": "^2.3.0",
    "graphql": "^0.13.1",
    "marked": "^0.3.6",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express-graphql": "^0.6.12",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.1.1"
  }
}
